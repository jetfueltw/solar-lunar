{
    "name": "jetfueltw/solarlunar-php",
    "description": "Chinese solar lunar calendar converter",
    "license": "Apache-2.0",
    "type": "library",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.5",
        "nesbot/carbon": "^2.27",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Jetfuel\\SolarLunar\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
